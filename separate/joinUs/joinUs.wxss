page{
  padding: 0;
  background: #fbfbfb;
}
.content-showImg image{
  width: 750rpx;
  height: 383rpx;
}
.content-form{
  padding-bottom: 50rpx;
  margin-top: -210rpx;
}
.content-form .list-item{
  margin-bottom: 20rpx;
  overflow: hidden;
  color: #333;
  position: relative;
}
.content-form .list-item input,
.content-form .list-item textarea{
  box-sizing: border-box;
  width: calc(100% - 100rpx);
  margin: 0 50rpx;
  height: 86rpx;
  font-size: 30rpx;
  line-height: inherit;
  outline: none;
  background-color: #fff;
  border: solid 1rpx #cccccc;
  border-radius: 15rpx;
  padding: 0 30rpx;
}
.content-form .list-item textarea{
  padding: 20rpx 30rpx;
  height: 180rpx;
}
.content-form .btn-submit {
  box-sizing: border-box;
  display: inline-block;
  width: calc(100% - 100rpx);
  margin: 30rpx 50rpx 0 50rpx;
  height: 86rpx;
  line-height: 86rpx;
  font-size: 36rpx;
  border-radius: 15rpx;
  text-align: center;
  text-decoration: none;
  color: #fff;
  background: #b4282d;
}
.imgContent{
  margin: 0 50rpx;
  overflow: hidden;
  color: #666;
  margin-bottom: 20rpx;
}
.imgContent .imgItem{
  float: left;
  width: 200rpx;
  border-radius: 15rpx;
  border: 1rpx solid #cccccc;
  box-sizing: border-box;
  height: 200rpx;
  background: #fff;
  position: relative;
}
.imgContent .imgItem.companyImg,
.imgContent .imgItem.cardImg_2{
  margin-left: 25rpx;
}
.imgContent .imgItem text{
  font-size: 26rpx;
  display: block;
  padding: 0 10rpx;
  box-sizing: border-box;
  text-align: center;
}
.imgContent .imgItem .icon{
  display: block;
  font-size: 80rpx;
  text-align: center;
}
.imgContent .imgItem input{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
}
.imgContent .imgItem image{
  width: 100%;
  height: 100%;
}
.joinUs-content .home{
  position: absolute;
  right: 10rpx;
  bottom: 150rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background: #fff;
  border: 1rpx solid #efefef;
}
.joinUs-content .home navigator{
  width: 100%;
  height: 100%;
}
.joinUs-content .home navigator image{
  width: 60rpx;
  height: 56rpx;
  padding: 10rpx 12rpx;
}