.alertdefault{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.8);
  z-index: 9999;
}
.alertdefault-content{
  width: 478rpx;
  height: 120rpx;
  background: #fff;
  border-radius: 16rpx;
  text-align: center;
  margin: 474rpx 136rpx;
  padding: 70rpx 0;
}
.alertdefault-title{
  color: #333;
  font-size: 28rpx;
  margin-bottom: 30rpx;
}
.alertdefault-msg{
  color: #bbb;
  font-size: 24rpx;
}